# https://leetcode.com/problems/valid-parentheses/
class Solution:
    def isValid(self, s: str) -> bool:
        open_closed_map = {
            "(": ")",
            "{": "}",
            "[": "]"
        }
        stack = []
        
        for bracket in s:
            if bracket in open_closed_map.keys():
                stack.append(bracket)
            else:
                if stack and open_closed_map[stack[-1]] == bracket:
                    stack.pop()
                else:
                    return False
                
        return True if len(stack) == 0 else False